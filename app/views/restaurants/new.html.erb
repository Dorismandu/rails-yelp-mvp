<h1>Add a new Restaurant</h1>

<%= simple_form_for(@restaurant) do |f| %>
    <%= f.input :name %>
    <%= f.input :category, as: :select, collection:  ["chinese", "italian", "japanese", "french", "belgian"] %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.submit "Save", class: "btn btn-primary" %>
<% end %>
  